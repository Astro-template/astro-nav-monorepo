    "clean": "turbo run clean"
  },
  "devDependencies": {
    "turbo": "^1.13.4",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
